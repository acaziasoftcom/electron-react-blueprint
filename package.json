{
  "name": "pos-app-react",
  "productName": "pos-app-react",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "app/index.js",
  "scripts": {
    "start": "NODE_ENV=development run-p webpack forge:start",
    "build": "run-s webpack:build forge:package",
    "forge:start": "electron-forge start",
    "forge:package": "electron-forge package",
    "make:mac": "electron-forge make --platform=darwin",
    "make:linux": "electron-forge make --platform=linux --arch=ia32,x64",
    "make:windows": "electron-forge make --platform=win32 --arch=ia32,x64",
    "publish": "electron-forge publish",
    "webpack": "webpack-dev-server --config webpack.dev.js",
    "webpack:build": "webpack --progress --config webpack.prod.js",
    "fixcode": "standard --fix",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "lint": "standard --verbose --color | snazzy",
    "git-hook": "npm run lint -s"
  },
  "keywords": [],
  "author": "Doko",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true,
        "arch": "all",
        "ignore": [
          ".vscode",
          ".cache",
          "__mocks__",
          "__tests__",
          "src",
          "template",
          "out",
          "docs",
          "screenshots"
        ]
      },
      "electronWinstallerConfig": {
        "name": "POS App React"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "acaziasoftcom",
        "name": "electron-react-blueprint"
      },
      "windowsStoreConfig": {
        "packageName": "com.acaziasoft.electron",
        "name": "posappreact"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sqlite3": "^4.0.4",
    "usb-detection": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "electron-forge": "^5.2.3",
    "electron-prebuilt-compile": "3.0.13",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^6.1.1",
    "sass-loader": "^7.1.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "/dist/",
      "**/out/",
      "/node_modules/",
      "/scripts/"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element",
      "alert",
      "string",
      "number"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run git-hook && npm run fixcode"
    }
  }
}
